    </span><span class="s2">"holding"</span><span class="p">:[</span><span class="w">
           </span><span class="p">{</span><span class="w">
              </span><span class="s2">"symbol"</span><span class="p">:</span><span class="s2">"cmt_btcusdt"</span><span class="p">,</span><span class="w"> </span><span class="err">//合约名称</span><span class="w">
              </span><span class="s2">"liquidation_price"</span><span class="p">:</span><span class="s2">"0.00"</span><span class="p">,</span><span class="w"> </span><span class="err">//预估爆仓价</span><span class="w">
              </span><span class="s2">"position"</span><span class="p">:</span><span class="s2">"0"</span><span class="p">,</span><span class="w"> </span><span class="err">//持仓数量</span><span class="w">
