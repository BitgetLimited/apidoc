   </span><span class="s2">"short_leverage"</span><span class="p">:</span><span class="s2">"2"</span><span class="p">,</span><span class="w"> </span><span class="err">//空仓杠杆</span><span class="w">
   </span><span class="s2">"forwardContractFlag"</span><span class="p">:</span><span class="kc">true</span><span class="w"> </span><span class="err">//是否是正向合约</span><span class="w">
