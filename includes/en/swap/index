         </span><span class="p">[</span><span class="w">
            </span><span class="s2">"8858.0"</span><span class="p">,</span><span class="w">   </span><span class="err">//price</span><span class="w">
            </span><span class="s2">"19299"</span><span class="p">,</span><span class="w">   </span><span class="err">//amount</span><span class="w">
              </span><span class="mf">2.0</span><span class="w">     </span><span class="err">//Obsolete</span><span class="w"> </span><span class="err">field</span><span class="w">
        </span><span class="p">]</span><span class="w">   
     </span><span class="p">],</span><span class="w">
   </span><span class="s2">"bids"</span><span class="p">:[</span><span class="w">
         </span><span class="p">[</span><span class="w">
            </span><span class="s2">"7466.0"</span><span class="p">,</span><span class="w">   </span><span class="err">//price</span><span class="w">
            </span><span class="s2">"499"</span><span class="p">,</span><span class="w">     </span><span class="err">//amount</span><span class="w">
             </span><span class="mf">1.0</span><span class="w">       </span><span class="err">//Obsolete</span><span class="w"> </span><span class="err">field</span><span class="w">
        </span><span class="p">],</span><span class="w">
