    </span><span class="s2">"resultHoldMode"</span><span class="p">:</span><span class="mi">1</span><span class="p">,</span><span class="w">        </span><span class="err">//Return</span><span class="w"> </span><span class="err">to</span><span class="w"> </span><span class="err">the</span><span class="w"> </span><span class="err">account</span><span class="w"> </span><span class="err">mode</span><span class="w"> </span><span class="mi">1</span><span class="w"> </span><span class="err">position</span><span class="w"> </span><span class="err">by</span><span class="w"> </span><span class="err">position</span><span class="w"> </span><span class="mi">2</span><span class="w"> </span><span class="err">full</span><span class="w"> </span><span class="err">position</span><span class="w">
    </span><span class="s2">"switchSuccess"</span><span class="p">:</span><span class="kc">true</span><span class="w">      </span><span class="err">//Whether</span><span class="w"> </span><span class="err">the</span><span class="w"> </span><span class="err">modification</span><span class="w"> </span><span class="err">is</span><span class="w"> </span><span class="err">successful</span><span class="w"> </span><span class="kc">true</span><span class="w"> </span><span class="err">success</span><span class="w"> </span><span class="kc">false</span><span class="w"> </span><span class="err">failure</span><span class="w">
