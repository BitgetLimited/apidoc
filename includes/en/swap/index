   </span><span class="s2">"long_leverage"</span><span class="p">:</span><span class="s2">"100"</span><span class="p">,</span><span class="w">           </span><span class="err">//Leverage</span><span class="w"> </span><span class="err">level</span><span class="w"> </span><span class="err">for</span><span class="w"> </span><span class="err">long</span><span class="w"> </span><span class="err">positions</span><span class="w">
   </span><span class="s2">"margin_mode"</span><span class="p">:</span><span class="s2">"fixed"</span><span class="p">,</span><span class="w">          </span><span class="err">//Margin</span><span class="w"> </span><span class="err">mode</span><span class="p">:</span><span class="w"> </span><span class="err">crossed</span><span class="w"> </span><span class="err">/</span><span class="w"> </span><span class="err">fixed</span><span class="w">
